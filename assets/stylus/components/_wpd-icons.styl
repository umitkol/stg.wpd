.fa-arrows-v-cross
  transform rotateZ(-45deg)
  padding-left 3px
  
  &:before
    content "\f07d"
    
.fa-shadow
  transform rotateZ(-180deg)
  &:before 
    content "\f24d"

    
.fa-picture-o.contained
  font-size 10px
  border 1px solid $color13
  width 15px
  position absolute
  left 50%
  margin-left -8px
  top 11px
  
.ico-gradient
  display inline-block
  height 10px 
  width 13px 
  border 1px solid $color13
  background linear-gradient(top, $color13, $color19)
  
.fa-custom-t:before
  content "T"
  
.fa-mobile-b
  transform rotateZ(90deg) 
  
  &:before 
    content "\f10b"

.wpd-i 
  display inline-block
   
  &--section
    retinaSprite($section_group) 
  
  &--container
    retinaSprite($container_group) 
  
  &--columns
    retinaSprite($columns_group) 
  
  &--div-block
    retinaSprite($div_block_group) 
  
  &--link-block
    retinaSprite($link_block_group) 
  
  &--button
    retinaSprite($button_group) 
  
  &--list
    retinaSprite($list_group) 
  
  &--list-item
    retinaSprite($list_item_group) 
  
  &--heading
    retinaSprite($heading_group) 
  
  &--paragraph
    retinaSprite($paragraph_group) 
  
  &--link
    retinaSprite($link_group) 
  
  &--text
    retinaSprite($text_group) 
  
  &--blockquote
    retinaSprite($blockquote_group)
   
  &--rich-text
    retinaSprite($rich_text_group)
  
  &--dynamic-list
    retinaSprite($dynamic_list_group)
  
  &--image
    retinaSprite($image_group)
  
  &--video
    retinaSprite($video_group)
  
  &--form-block
    retinaSprite($form_block_group)
  
  &--label
    retinaSprite($label_group)
  
  &--input
    retinaSprite($input_group) 
  
  &--text-area
    retinaSprite($text_area_group)
  
  &--checkbox
    retinaSprite($checkbox_group) 
  
  &--radio
    retinaSprite($radio_group)
   
  &--select
    retinaSprite($select_group)
  
  &--submit
    retinaSprite($submit_group)
  
  &--slider
    retinaSprite($slider_group)
  
  &--tabs
    retinaSprite($tabs_group)
  
  &--map
    retinaSprite($map_group)
  
  &--facebook
    retinaSprite($facebook_group)
  
  &--twitter
    retinaSprite($twitter_group)
    
.wp-i--global
  color $color23
     
.wp-i--mobile
  &.landscape
    transform rotateZ(90deg) 

.wp-i--t-normal
  font-family 'Times New Roman' !important
  font-size 16px
  margin-top -8px
  font-weight bold
  
  &:before 
    content "I"
    
.wpd-button .wp-i--t-normal
  font-size 16px
    
.wp-i--t-italic
  font-family 'Times New Roman' !important
  font-size 16px
  margin-top -8px
  font-weight bold
  font-style italic
  
  &:before 
    content "I"

.wp-i.wp-i--font-size
  font-size 14px


.wpd-top-bar
  .wpd-button-group
    .wp-i.wp-i--laptop 
      display inline-block
      font normal normal normal 14px/1 FontAwesome !important
      font-size inherit
      text-rendering auto
      -webkit-font-smoothing antialiased
      -moz-osx-font-smoothing grayscale
      margin-top -8px !important
      font-size 20px !important
      
      &:before 
        content "\f108"
        
    .wp-i.wp-i--tablet
      display inline-block
      font normal normal normal 14px/1 FontAwesome !important
      font-size inherit
      text-rendering auto
      -webkit-font-smoothing antialiased
      -moz-osx-font-smoothing grayscale
      margin-top -11px !important
      font-size 24px !important
      
      &:before
        content: "\f10a";

        
    .wp-i.wp-i--mobile
      display inline-block
      font normal normal normal 14px/1 FontAwesome !important
      font-size inherit
      text-rendering auto
      -webkit-font-smoothing antialiased
      -moz-osx-font-smoothing grayscale
      font-size 30px !important
      margin-top -14px !important
      
      &:before
        content: "\f10b";

        
    .wp-i.wp-i--mobile-b
      display inline-block
      font normal normal normal 14px/1 FontAwesome !important
      font-size inherit
      text-rendering auto
      -webkit-font-smoothing antialiased
      -moz-osx-font-smoothing grayscale
      margin-top -7px !important
      transform rotateZ(-90deg) 
      font-size 30px !important
      margin-top -14px !important
      
      &:before
        content: "\f10b";

.wpd-button-group--narrow
  .wpd-button .wp-i
    font-size 15px
    margin-top -8px
    
.wpd-button-group--radius-handler
  .wpd-button .wp-i
    font-size 10px
    margin-top -5px